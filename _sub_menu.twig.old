<nav class="navbar navbar-custom navbar-static-top">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">MATTRONIX</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

	              <ul class="nav navbar-nav">
                {% macro display_menu_item(item, loop) %}
                  {% from _self import display_menu_item %}
                    <li class='{% if item|current %}active{% endif %}'>
                    <a href="{{ item.link }}" {% if item.title is defined %}title='{{ item.title|escape }}'{% endif %}'>
                    {% if item.label is defined %}{{item.label}}{% else %} - {% endif %} </a></li>
                {% endmacro %}

                {% from _self import display_menu_item %}
                  {% for item in menu %}
                  {% if item.label is defined %}
                    {{ display_menu_item(item, loop) }}
                  {% endif %}
                {% endfor %}
              </ul>

	              {% include '_sub_searchbox.twig' %}
